Statement AmiBlitz3_Unprepare{docTypeX.l}
  !_ASSERT{DocType_IsAvailable{docTypeX}}
  ctx.l = AIDE\DocType[docTypeX]\Context

  StartUp_Message{"unprepare AmiBlitz3 environment..."}
  If AB3_Ctx(ctx)\TempP   ><Null Then FreeVec_ AB3_Ctx(ctx)\TempP        : AB3_Ctx(ctx)\TempP  = Null
  If AB3_Ctx(ctx)\TokensT ><Null Then pft_Free{AB3_Ctx(ctx)\TokensT}     : AB3_Ctx(ctx)\TokensT = Null
  If AB3_Ctx(ctx)\SDK     ><Null Then pft_Free{AB3_Ctx(ctx)\SDK}         : AB3_Ctx(ctx)\SDK  = Null
  If AB3_Ctx(ctx)\Tokens  ><Null Then hashmap_Free{AB3_Ctx(ctx)\Tokens}  : AB3_Ctx(ctx)\Tokens = Null
  If AB3_Ctx(ctx)\LTokens ><Null Then hashmap_Free{AB3_Ctx(ctx)\LTokens} : AB3_Ctx(ctx)\LTokens = Null
  If AB3_Ctx(ctx)\Detokens><Null Then FreeVec_ AB3_Ctx(ctx)\Detokens     : AB3_Ctx(ctx)\Detokens = Null

  AIDE\DocType[docTypeX]\IsPrepared = False

End Statement


AmiBlitz3_Prepare:
Function.l AmiBlitz3_Prepare{docTypeX.l}
  !_ASSERT{DocType_IsAvailable{docTypeX}}
  If (AmiBlitz3\IsInit = False) Then AmiBlitz3_InitCharMap{}
  ctx.l = AIDE\DocType[docTypeX]\Context

  !_ASSERT{AIDE\DocType[docTypeX]\IsPrepared = False}

  StartUp_Message{"prepare AmiBlitz3 environment..."}

  AB3_Ctx(ctx)\TempP = AllocVec_(1024,#MEMF_ANY)
  AB3_Ctx(ctx)\Tokens  = hashmap_Load{AB3_Ctx(ctx)\TokenHashMapFile}
  If AB3_Ctx(ctx)\Tokens = Null ; SDK was not scanned
    Default_Prepare{docTypeX}
    Function Return True
  End If
  AB3_Ctx(ctx)\LTokens = hashmap_Load{AB3_Ctx(ctx)\TokenHashMapFileL}
  AB3_Ctx(ctx)\TokensT = pft_Load{AB3_Ctx(ctx)\TokenTreeFile}
  AB3_Ctx(ctx)\SDK  = pft_Load{AB3_Ctx(ctx)\SdkFile}

  If AB3_Ctx(ctx)\Tokens ; generate inverse mapping from libid/tokenid => token name
    If AB3_Ctx(ctx)\Detokens=Null
      AB3_Ctx(ctx)\Detokens = AllocVec_ (128*256*4,#MEMF_CLEAR)
    Else
      For n.l=0 To 128*256-1
        Poke.l AB3_Ctx(ctx)\Detokens + n*4,Null
      Next
    End If

    *hm.hashmap = AB3_Ctx(ctx)\Tokens
    *ta.l       = AB3_Ctx(ctx)\Detokens
    If *ta><Null AND *hm><Null
      For n.l=0 To *hm\size-1
        ptr.l   = Peek.l(*hm\base + n*4)
        If ptr
          size.l   = Peek.l(ptr) : ptr +4
          endptr.l = ptr + size
          While ptr<endptr
            ikl.l = Peek.l(ptr) : ptr +4+ikl
            isl.l = Peek.l(ptr) : ptr +4+isl
            tokenIndex.l = (Peek.w(ptr-isl) & $7FFF)
            Poke.l *ta+tokenIndex*4,ptr-isl-4-ikl-4
          Wend
        End If
      Next
    End If
  End If

  _ab3_LoadBlitzLibNames{docTypeX, AB3_Ctx(ctx)\BlitzLibNameFile}
  Default_Prepare{docTypeX}
  Function Return True

End Function
!_MakeGlobalFuncPointer{AmiBlitz3_Prepare,{0}}


