Function.l _ab3_JumpToSDKItemInfo{info.s}

  If info = "" Then Function Return False
  source.s = _ab3_GetInfoSource{info}
  line_.s  = _ab3_GetInfoLine{info}
  lpos.l   = Vallong(cropfield{line_,0,@"/",@"/"})
  cpos.l   = Vallong(cropfield{line_,1,@"/",0})
  ex.s = dos_GetExt{source}
  If ex = "ab3" OR ex = "ab2" OR ex = "bb2"
    docX.l   = Document_Open{source}
    If lpos>0 Then lpos-1
    If cpos>0 Then cpos-1
    Document_JumpTo{docX,lpos,cpos,True}
    Function Return True
  End If

  Function Return False

End Function


AmiBlitz3_JumpTo:
Statement AmiBlitz3_JumpTo{docTypeX.l, docX.l,*typeP.b,*nameP.b,*moreP.b}

  If *nameP Then name.s  = Peek.s(*nameP)
  If *typeP Then type_.s = Peek.s(*typeP)
  If *moreP Then more.s  = Peek.s(*moreP)

  *local.prefixtree = Null
  ctx.l = DocType_GetContext{docTypeX}

  If docX>=0
    *xtra.AmiBlitz3_Xtra = (.AmiBlitz3_Xtra)AIDE\Document[docX]\Xtra
    If *xtra
      *local  = *xtra\LocalSDK
    End If
  End If

  Select type_
    Case "C"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"CONSTANT",UnRight$(name,1)}
    Case "NT" : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"NEWTYPE" ,UnRight$(name,1)}
    Case "M"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"MACRO"   ,UnRight$(name,1)}
    Case "L"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"LABEL"   ,UnRight$(name,1)}
    Case "G"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"GLOBAL"  ,name}
    Case "V"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"LOCAL"   ,name}
    Case "@"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"TOKEN"   ,name}      ; call the guide!
    Case "$"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"STRING"  ,more}
    Case "F"  : info.s = getSdkItemInfo{*local,AB3_Ctx(ctx)\SDK,"FUNCTION",name}
    Default
      info.s  = ""
      error{"Unknown entity to jump to : "+type_+"!"}
  End Select

  _ab3_JumpToSDKItemInfo{info}

End Statement
!_MakeGlobalFuncPointer{AmiBlitz3_JumpTo,{0,0,Null,Null,Null}}


