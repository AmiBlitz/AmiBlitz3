        help.s      = cropfield{&a,2,seperator,$00}
        addSdkItem{*sdk,id,newtype_,sourcefile+"\\09"+codeline+"\\09"+help}

      Case "FIELD"    :
        codeline.s  = "0"
        field.s     = cropfield{&a,1,seperator,@"."}
        type_.s     = cropfield{&a,1,@".",seperator}
        help.s      = cropfield{&a,2,seperator,$00}
        addSdkItem{*sdk,id,newtype_+"\"+field,sourcefile+"\\09"+codeline+"\\09"+type_+"\\09"+help}

      Case "MACRO"    :
        codeline.s  = "0"
        macro_.s    = cropfield{&a,1,seperator,seperator}
        help.s      = cropfield{&a,2,seperator,$00}
        addSdkItem{*sdk,id,macro_,sourcefile+"\\09"+codeline+"\\09"+help}

      Case "CONSTANT" :
        codeline.s  = "0"
        constant.s  = cropfield{&a,1,seperator,seperator}
        value.s     = cropfield{&a,2,seperator,seperator}
        help.s      = cropfield{&a,3,seperator,$00}
        addSdkItem{*sdk,id,constant,sourcefile+"\\09"+codeline+"\\09"+value+"\\09"+help}

    End Select

  Wend
  file_Close{fid}
  Function Return True

End Function


; scan the resident files and put content to sdk
Function.l _ab3_ScanResidents{docTypeX.l, *sdk.prefixtree, temp.s}

  ctx.l = DocType_GetContext{docTypeX}
  residentdir.s = dos_AddPart{AB3_Ctx(ctx)\Blitz3Path, "BlitzLibs/Residents"}
  Repeat
    resident.s = dos_ScanDir{residentdir,"#?.res",#dos_scan_files}
    resident = dos_FilePart{resident}
    If resident
      StartUp_Message{"query "+resident+" from AmiBlitz3..."}
      tokenfile.s  = dos_AddPart{temp,"res_"+resident+".txt"}
      sourcefile.s = dos_AddPart{temp,"res_"+resident+".ab3"}
      fid.l = file_Open{sourcefile, #file_forcewrite}
      If fid>=0
        file_WriteLine{fid,"; XTRA"}
        file_WriteLine{fid,"; Residents = \\22"+resident+"\\22"}
        file_WriteLine{fid,"; /XTRA"}
        file_WriteLine{fid,"End"}
        file_Close{fid}
        dos_RunCli{AB3_Ctx(ctx)\Exe,0,-1,True,"-source \\22"+sourcefile+"\\22 -dump macro|type|const \\22"+tokenfile+"\\22"}
        _ab3_ImportAb3DumpFileSDK{docTypeX,tokenfile,*sdk,resident}
        CNIF #AB3_CLEANUP_TEMP
        dos_Delete{tokenfile}
        dos_Delete{sourcefile}
        CEND
      End If
    End If
  Until resident = ""

  Function Return True

End Function


AmiBlitz3_ScanSDK:
Function.l AmiBlitz3_ScanSDK{docTypeX.l}

  ctx.l = DocType_GetContext{docTypeX}
  StartUp_Message{"setup AmiBlitz3 SDK index..."}

  exe.s = dos_AddPart{AIDE\DocType[docTypeX]\SDKPath, "AmiBlitz3"}
  If _AmiBlitz3_ReadEnvironment{docTypeX,exe} = False
    exeb.s = dos_AddPart{AIDE\DocType[docTypeX]\SDKPath, "AmiBlitz3_Beta"}
    If _AmiBlitz3_ReadEnvironment{docTypeX,exeb} = False
      error{"Unable to locate AmiBlitz3 executable. Tried:\\n"+exe+"\\nand\\n"+exeb+"\\nPlease go to \\22Preferences/File Types\\22 and set the  SDK path."}
      ntui_PostNotify{*ntui,"prefs_opengui",Null}
      Function Return False
    End If
  End If
  temp.s = dos_AddPart{AB3_Ctx(ctx)\AIDEDir,"temp"}
  dos_CreateDir{temp}

  tokenfile.s      = dos_AddPart{temp,"tokens.txt"}
  includesfile.s   = dos_AddPart{temp,"includes.txt"}
  dos_Delete{tokenfile}
  dos_Delete{includesfile}

  *tokens.hashmap = hashmap_Create{14}
  *ltokens.hashmap = hashmap_Create{14}
  *tokenT.prefixtree = pft_Create{4096}
  If *tokens = Null OR *ltokens = Null OR *tokenT = Null Then Function Return False

  *sdk.prefixtree = pft_Create{1024*1024}
  If *sdk = Null Then Function Return False

  StartUp_Message{"query Blitz Tokens from AmiBlitz3..."}
  dos_RunCli{AB3_Ctx(ctx)\Exe,0,-1,True,"-dump token \\22"+tokenfile+"\\22"}
  _ab3_ImportAb3DumpFileTokens{docTypeX,tokenfile,*tokens,*ltokens,*tokenT}

  StartUp_Message{"import BlitzLib mapping..."}
  tokenmapfile.s = dos_AddPart{AB3_Ctx(ctx)\Blitz3Path,"System/token_mapping_libs.txt"}
  _ab3_ImportToken2LibFile{docTypeX,tokenmapfile,*tokens}

  StartUp_Message{"import Blitz Tokens to SDK..."}
  _ab3_ImportAb3DumpFileSDK{docTypeX,tokenfile,*sdk,""}

  StartUp_Message{"scan AmiBlitz3 includes..."}
  ab3scan.s = dos_AddPart{AB3_Ctx(ctx)\AIDEDir,"AB3Scan"}

  ; blocking
  ;dos_RunCli{ab3scan,0,-1,True,">"+includesfile+" "+AB3_Ctx(ctx)\IncludeDir+" -r -df SFMTNCGL"}

  ; asnyc
  params.s = "\\22"+AB3_Ctx(ctx)\IncludeDir+"\\22 -r -df SFMTNCGL"
  If ntui_RunAsyncAndNotify{*ntui,ab3scan,params,includesfile,"SCANDONE",0,""}
    done.l = False
    Repeat
      *notify.tuiNotify = ntui_WaitNotify{*ntui}
      Select ntui_GetNotifyID{*notify}
        Case "ev_splash_tick" : StartUp_Message{""}
        Case "SCANDONE" : done=True
      End Select
    Until done
    StartUp_Message{"import AmiBlitz3 include scan..."}
    _ab3_ImportAb3ScanFile{docTypeX,includesfile,*sdk,$09}
  Else
    error{"Unable to run "+ab3scan+" "+params+"!"}
  End If

  StartUp_Message{"scan AmiBlitz3 residents..."}
  _ab3_ScanResidents{docTypeX, *sdk, temp}

  StartUp_Message{"sorting AmiBlitz3 SDK database..."}
  pft_Sort{*sdk}

  StartUp_Message{"writing AmiBlitz3 SDK database..."}
  hashmap_Save{*tokens,AB3_Ctx(ctx)\TokenHashMapFile}
  hashmap_Save{*ltokens,AB3_Ctx(ctx)\TokenHashMapFileL}
  pft_Save{*tokenT,AB3_Ctx(ctx)\TokenTreeFile}
  pft_Save{*sdk,AB3_Ctx(ctx)\SdkFile}
  _ab3_SaveBlitzLibNames{docTypeX,AB3_Ctx(ctx)\BlitzLibNameFile}

  pft_Free{*tokenT}
  pft_Free{*sdk}
  hashmap_Free{*tokens}
  hashmap_Free{*ltokens}

  CNIF #AB3_CLEANUP_TEMP
  dos_Delete{tokenfile}
  dos_Delete{includesfile}
  CEND

  Function Return True

End Function
