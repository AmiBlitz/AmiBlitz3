;/* event handler function */
AmiBlitz3_HandleNotify:
Function.l AmiBlitz3_HandleNotify{*notify.tuiNotify}

  Select ntui_GetNotifyID{*notify}
    Case !ev_ab3_jumpto
      docTypeX.l = BaseType_GetFirstDocType{#BASETYPE_AMIBLITZ3}
      If DocType_IsAvailable{docTypeX} = False Then Function Return True
      *lv.tuiListView = (.tuiListView)ntui_GetObjectByID{*ntui,!obj_ab3_list}
      If *lv = Null Then Function Return True
      i.l     = ntui_GetFirstSelectedListItem{*lv}
      type_.s = ntui_GetListItemText{*lv,i,0}
      name.s  = ntui_GetListItemText{*lv,i,1}
      more.s  = ntui_GetListItemText{*lv,i,2}
      AmiBlitz3_JumpTo{docTypeX, -1, &type_, &name, &more}

    Case !ev_ab3_refresh
      docTypeX.l = BaseType_GetFirstDocType{#BASETYPE_AMIBLITZ3}
      If DocType_IsAvailable{docTypeX} = False Then Function Return True
      AmiBlitz3_SDKBrowser{docTypeX}

    Case !ev_ab3_rescan
      docTypeX.l = BaseType_GetFirstDocType{#BASETYPE_AMIBLITZ3}
      If DocType_IsAvailable{docTypeX} = False Then Function Return True
      ntui_ShowWindowByID{*ntui,!win_splash} ; show splash window because this can take a while
      AmiBlitz3_ScanSDK{docTypeX}
      AmiBlitz3_Unprepare{docTypeX}
      AmiBlitz3_Prepare{docTypeX}
      id.s = !ev_ab3_refresh
      ntui_PostNotify{*ntui,&id}
      ntui_HideWindowByID{*ntui,!win_splash}

    Default
      Function Return False; not interested

  End Select

  Function Return True
End Function
!_MakeGlobalFuncPointer{AmiBlitz3_HandleNotify,{Null}}

AmiBlitz3_Register{#BASETYPE_AMIBLITZ3}
InstallNotifyHandler{"AmiBlitz3", *AmiBlitz3_HandleNotify}

