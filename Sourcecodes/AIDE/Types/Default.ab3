Statement Default_Register{baseTypeX.l}

  AIDE\BaseType[baseTypeX]\IsAvailable   = True
  AIDE\BaseType[baseTypeX]\Name          = "Default"
  AIDE\BaseType[baseTypeX]\InitDocType   = *Default_InitDocType
