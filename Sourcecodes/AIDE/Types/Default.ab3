Default_Attach:
Function.l Default_Attach{docTypeX.l, docX.l}

  AIDE\Document[docX]\DocTypeX = docTypeX
  ntui_SetAttr{AIDE\Document[docX]\TextBox,#TUITBA_STYLECB,AIDE\DocType[docTypeX]\StyleCallback}

  Function Return True

End Function
!_MakeGlobalFuncPointer{Default_Attach,{0, 0}}


Default_Detach:
Function.l Default_Detach{docTypeX.l, docX.l}

  AIDE\Document[docX]\DocTypeX = #DOCTYPE_DEFAULT
  ntui_SetAttr{AIDE\Document[docX]\TextBox,#TUITBA_STYLECB,AIDE\DocType[#DOCTYPE_DEFAULT]\StyleCallback}

  Function Return True

End Function
!_MakeGlobalFuncPointer{Default_Detach,{0, 0}}


Default_Import:
Function.l Default_Import{docTypeX.l, docX.l, *filenameP.b}

  filename.s = Peek.s(*filenameP)
  *textBox.tuiTextBox = AIDE\Document[docX]\TextBox
  err.l = ntui_LoadTextBox{*textBox,&filename}
  
  Function Return (err = 0)

End Function
!_MakeGlobalFuncPointer{Default_Import,{0,0,Null}}


Default_Export:
Function.l Default_Export{docTypeX.l, docX.l, *filenameP.b}

  filename.s = Peek.s(*filenameP)
  *textBox.tuiTextBox = AIDE\Document[docX]\TextBox
  err.l = ntui_SaveTextBox{*textBox,&filename}
  
  Function Return (err = 0)

End Function
!_MakeGlobalFuncPointer{Default_Export,{0,0,Null}}


