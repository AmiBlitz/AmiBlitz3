NPrint "ChipSet: " + Peek$(IdHardware_(#IDHW_CHIPSET,0))
NPrint "GFX System: " + Peek$(IdHardware_(#IDHW_GFXSYS,0))
NPrint "ChipRam: " + Peek$(IdHardware_(#IDHW_CHIPRAM,0))
NPrint "FastRam: " + Peek$(IdHardware_(#IDHW_FASTRAM,0))
