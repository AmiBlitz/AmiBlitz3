WBStartup
;NoCli


; *** Prints system informations... ***
NPrint "SystemInfo - (c)Luca Ferraris"
NPrint "Written using Blitz Basic2 and Identify.library" + Chr$(10)

NPrint "System: " + Peek$(IdHardware_(#IDHW_SYSTEM,0))
NPrint "CPU: " + Peek$(IdHardware_(#IDHW_CPU,0))
NPrint "FPU: " + Peek$(IdHardware_(#IDHW_FPU,0))
NPrint "MMU: " + Peek$(IdHardware_(#IDHW_MMU,0))
