; --
; -- this little example shows the use of USEPATH, USEPATH2 and LASTUSEPATH
; --


NEWTYPE .te
  te.l
End NEWTYPE

test1.te\te=1
test2.te\te=2
test3.te\te=3
USEPATH test1
USEPATH2 test2

NPrint \te," should be 1"
NPrint \\te," should be 2"
USEPATH test2
USEPATH test3
NPrint \te," should be 3"
USELASTPATH
NPrint \te," should be 2"
USELASTPATH
NPrint \te," should be 1"
MouseWait
