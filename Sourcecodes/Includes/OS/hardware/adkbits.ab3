#ADKF_USE1V2=(1 LSL 1)
#ADKF_USE0V1=(1 LSL 0)

#ADKF_PRE000NS=0;/* 000 ns of precomp */
#ADKF_PRE140NS=(#ADKF_PRECOMP0);/* 140 ns of precomp */
#ADKF_PRE280NS=(#ADKF_PRECOMP1);/* 280 ns of precomp */
