quitme.l = False
While quitme=False
  Wait_ $FFFFFFFF

  !tuiev_WhileEvent{*tuiEngine}
    Select !tuiev_Notify
      Case #NOTIFY_TEST     : ;message{"User hit the button!"}
      Case #NOTIFY_QUIT     : quitme=True
      Case #NOTIFY_CLOSE    : quitme=True
      Case #NOTIFY_POPUP    : message{"App pop up!"}
      Case #NOTIFY_ICONIFY  : message{"App iconify!"}
      Case #NOTIFY_DROP     : message{"file drop on window: "+!tuiev_String }
      Case #NOTIFY_ICONDROP : message{"file drop on app icon: "+!tuiev_String }
      Case #NOTIFY_AREXX    : message{"got AREXX message: "+!tuiev_String}
      Case #NOTIFY_Cycle
        a.s = Str$(ntui_GetValueByID{*tuiEngine,@"CYCL"})
        b.s = Str$(!tuiev_Value)
        message{"Cycle Event "+b.s+" and extra by ID "+a.s}
      Case @"VIEW"
        ;message{"Click view!"}
      Case #NOTIFY_REFRESH
        *tui.tuiObject = !tuiev_TuiObject
        If *tui
          Select *tui\userID
            Case @"VIEW"
              r.l = r+1
              *rp.RastPort = *tui\tuiWindow\win\RPort
              x.l = *tui\pos\x
              y.l = *tui\pos\y
              w.l = *tui\size\x
              h.l = *tui\size\y
              For y1.l=0 To h-1 Step 10
                For x1.l=0 To w-1 Step 10
                  n.l = (y1+x1)/10 +r
                  If (n&1)=1
                    SetAPen_ *rp,*tuiEngine\pen[#TUIPEN_HALFSHADOW]
                  Else
                    SetAPen_ *rp,*tuiEngine\pen[#TUIPEN_HALFSHINE]
                  End If
                  x2.l = x1+9 : If x2>=w Then x2=w-1
                  y2.l = y1+9 : If y2>=h Then y2=h-1
                  RectFill_ *rp,x1+x,y1+y,x2+x,y2+y
                Next
              Next
          End Select
        End If
    End Select
  !tuiev_WendEvent
Wend

ntui_FreeEngine{*tuiEngine}

