;/* This is the read/write operation structure -- very basic */
NEWTYPE.SDL_RWops
;/* Seek to 'offset' relative to whence, one of stdio's whence values:
;   SEEK_SET, SEEK_CUR, SEEK_END
;   Returns the final offset in the data source.
*_seek.l ; (struct SDL_RWops *context, Int offset, Int whence);

;/* Read up to 'num' objects each of size 'objsize' from the data
;   source to the area pointed at by 'ptr'.
;   Returns the number of objects read, or -1 if the read failed.
*_read.l ; (struct SDL_RWops *context, void *ptr, int size, int maxnum);

;/* Write exactly 'num' objects each of size 'objsize' from the area
;   pointed at by 'ptr' to data source.
;   Returns 'num', or -1 if the write failed.
*_write.l ; (struct SDL_RWops *context, const void *ptr, int size, int num);

;/* Close and free an allocated SDL_FSops structure */
*_close ; (struct SDL_RWops *context);

_type.l
hidden.b[8]
End NEWTYPE


