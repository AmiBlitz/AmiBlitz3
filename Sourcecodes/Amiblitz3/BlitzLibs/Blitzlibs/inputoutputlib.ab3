RTS

writeA0ToOutputWithCheck:
  MOVEM.l D0-D3/A1,-(A7)

  MOVE.l A0,D2
  MOVEQ #-1,D3
  LAB_0015:
    ADDQ.l #1,D3
    TST.b (A0)+
