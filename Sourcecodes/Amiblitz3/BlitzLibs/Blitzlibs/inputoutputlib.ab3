writeToOutput:
  MOVEM.l D1-D3/A0-A1,-(A7)
  MOVE.b D0,writeBuffer
  MOVE.l outputFileHandle(PC),D1   ;file
  MOVE.l #writeBuffer,D2           ;buffer
  MOVEQ #1,D3                      ;length
  MOVEA.l _dosBase(PC),A6
  JSR _Write(A6)                   ; -48
  MOVEM.l (A7)+,D1-D3/A0-A1
RTS

writeA0ToOutput:                                                                                                                                                                                                                                         
  MOVEM.l D1-D3/A1,-(A7)
