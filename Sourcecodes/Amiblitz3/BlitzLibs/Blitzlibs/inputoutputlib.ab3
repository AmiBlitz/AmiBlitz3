readFromInput:
  MOVEM.l D1-D3/A1,-(A7)
  MOVE.l A0,D2                  ; buffer
  MOVE.l D0,D3                  ; length
  MOVE.l inputFileHandle(PC),D1 ; file
  MOVEA.l _dosBase(PC),A6
  ADDQ.l #1,D3
  JSR _Read(A6)    ; -42
