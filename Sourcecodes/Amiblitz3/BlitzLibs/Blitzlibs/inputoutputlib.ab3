  TST.w D3
  BEQ.w LAB_0016
    MOVE.l outputFileHandle(PC),D1
    MOVEA.l _dosBase(PC),A6
    JSR _Write(A6)                   ; -48
  LAB_0016:
  MOVEM.l (A7)+,D0-D3/A1
RTS

