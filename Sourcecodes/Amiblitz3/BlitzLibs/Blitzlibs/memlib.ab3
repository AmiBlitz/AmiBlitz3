  TST.w d0 : BMI 'l2
    MOVE.l 2(a0),d0
    ADD.l #9,d0
    TRAP #0
    BRA 'l3
 'l2:
    MOVE.l #mungtext,d0
    TST.b debug : BNE 'l10
      TRAP #11
      BRA 'l3
   'l10:
      TRAP #0
