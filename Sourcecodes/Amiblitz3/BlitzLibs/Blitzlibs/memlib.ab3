  ;MOVE.l $0,a0 ;show enforcer hit
  MOVE.l a1,a0
  ADD.w #16,a0
  MOVE.l (a0),a0
  MOVEQ #30,d0
 'l1:
    CMP.w #$4e41,-(a0)      ; TRAP #1 ?
