.do_newmemnode:                    ;create a new node (for local stuff)    AJL_0_1EA
  MOVEM.l D0-D1/A0-A1/A6,-(A7)
  MOVEQ.l #20,D0            ; 20bytes
  MOVEQ.l #$01,D1           ; MEMF_PUBLIC
  MOVEA.l _SysBase,A6

  TST.w kick2_available : BNE 'pooling
    JSR _AllocMem(A6)
    BRA 'poolend
 'pooling
    MOVE.l mempool,a0
    JSR _AllocPooled(a6)
 'poolend

  TST.l D0 : BEQ.w outOfMemoryb

  MOVEA.l D0,A0
  MOVE.l memlist(PC),(A0)
  MOVE.l A0,memlist
  MOVE.l #$e0e0e0e0,16(a0)
  ;CLR.l $4(A0)
  ADDQ.l #4,a0
  !init
  MOVEM.l (A7)+,D0-D1/A0-A1/A6
RTS

.do_freememnode:                           ;free up most recent node, return eq=1 if none!  AJL_0_218
  MOVEM.l D0-D1/A0-A2/A6,-(A7)
  MOVEA.l _SysBase,A6
  MOVE.l memlist(PC),D0 : BEQ.w exit_do_freememnode

  MOVEA.l D0,A1
  MOVE.l (A1),memlist
  MOVE.l a1,-(a7)
  MOVE.l 4(a1),a2

  JL_0_23C:
  CMP.l #$0,(a2) : BEQ.w JL_0_254

  MOVEA.l A2,A1
  CMP.l #52,12(a2) : BLE 'nopool
    CMP.l #"lIsT",36+16(a1) : BNE 'nopool          ;free pool from lists
      CMP.w #1,6+16(a1) : BNE 'nopool
        MOVE.l 0+16(a1),a0
        CMP.l #0,a0 : BEQ 'nopool
          MOVE.l a1,-(a7)
          JSR _DeletePool(a6)
          MOVE.l (a7)+,a1
 'nopool:
  MOVE.l 12(A2),D0 ;was 4
  MOVEA.l (A2),A2

  TST.w kick2_available : BNE 'pooling
    JSR _FreeMem(A6)
    BRA poolingend
 'pooling
    CNIF #mungwall=1
      MOVE.l a1,a0
      ADD.w #20,a0
      ;CMP.l #$DEADDEAD,(a0)+ : BNE munghit2
      CMP.l #$EADDEADA,(a0)+ : BNE munghit2
      CMP.l #$ADDEADAD,(a0)+ : BNE munghit2
      CMP.l #$DAEAD987,(a0)+ : BNE munghit2
      
      MOVE.l a0,d1
      mungr2:
      MOVE.l a1,a0
      SUB.w #16,a0
      ADD.l d0,a0
      CMP.l #$DEADDEAD,(a0)+ : BNE munghit3
      CMP.l #$EADDEADA,(a0)+ : BNE munghit3
      CMP.l #$ADDEADAD,(a0)+ : BNE munghit3
      CMP.l #$DAEAD987,(a0)+ : BNE munghit3
      mungr3:
      MOVE.l d1,a0
      MOVE.l #$abcdefcd,(a0)+
      MOVE.l #$bcdefefe,(a0)+
      MOVE.l #$ccdefefe,(a0)+
    CEND
    MOVE.l mempool,a0
    CMP.l #"ChIp",8(a1) : BNE 'l1
      MOVE.l mempoolchip,a0
   'l1
    JSR _FreePooled(a6)
  poolingend:

  TST.l (a2) : BNE.w JL_0_23C

  JL_0_254:
  MOVE.l (a7)+,a1
  MOVEQ.l #20,D0

  TST.w kick2_available : BNE 'pooling
    JSR _FreeMem(A6)            ; (a1:memoryBlock, d0:byteSize)
    BRA 'poolend
 'pooling
    MOVE.l mempool,a0
    JSR _FreePooled(a6)
 'poolend
  MOVEQ.l #-$01,D0

  exit_do_freememnode:
  MOVEM.l (A7)+,D0-D1/A0-A2/A6
RTS

debug: Dc.w 0

outOfMemoryb:
CNIF #return0=1
  MOVEM.l (A7)+,D0-D1/A0-A2/A6
  RTS
