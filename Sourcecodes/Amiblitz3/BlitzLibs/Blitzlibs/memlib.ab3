memlist:  Ds.l 1
AL_0_EE:  Ds.w 1

;-- OK, here's how our mem linking works:
;--
;-- firstdata.l - points to current list header
;--
;-- listheader - 8 bytes.
;-- 00.l - pointer to next listheader
;-- 04.l - first memblock
;-- 08.l - last memalloc address
;--
;-- Memblocks are 8 bytes longer than normal
;--
;-- 00.l - Next
;-- 04.l - length of memblock + 8
;-- 08.l - CHIP or fast indicator
;-- 12.l - free
;-- 16...  Mem returned to User.
;--
;-- sorta like this:
;--
;-- firstdata ---list---list---list...
;--                !      !      !
;--               mem    mem    mem
;--                !      !      !
;--               mem    mem    mem
;--                :      :      :
;--                :      :      :
;--
;-- first list is always current local one.

;******************** CODE SECTION ********************
.allocret:                          ; allocate some memory in the local region!
  MOVEM.l A0-A1/A6,-(A7)

  BTST #0,D0 : BEQ 'notodd          ; check if memory is odd and add 1 byte
    ADDQ.l #1,D0
 'notodd

CNIF #mungwall=1
  ADD.l #16+16*2,d0
CELSE
  ADD.l #16,D0
CEND

  MOVE.l D0,-(A7)                   ; requested memsize + management data
  MOVEA.l _SysBase,A6
                                    ; CRASH ONLY WITH KICK 1.3 !!!
  TST.w kick2_available : BNE 'pooling
    JSR _AllocMem(A6)
    BRA 'poolend
 'pooling
    BTST #1,d1 : BNE 'chipm
      MOVE.l mempool,a0
      JSR _AllocPooled(a6)
 'poolend

  TST.l D0 : BEQ.w 'l1
    MOVEA.l D0,A0
    MOVEA.l memlist(PC),A1
    ADDQ.l #4,a1
    !addtail
    ;  MOVE.l $4(A1),(A0)
    ;  MOVE.l d0,8(a1)
    ;  MOVE.l A0,$4(A1)
    MOVE.l (A7)+,12(A0)             ;size was 4, requested memsize+management data

CNIF #mungwall=1
    MOVE.l a0,a1
    SUB.w #16,a1
    ADD.l 12(a0),a1        ; add of size
    ADD.w #16,a0
    ;MOVE.l #$DEADDEAD,(a0)+
    MOVE.l 28(a7),(a0)+
    MOVE.l #$EADDEADA,(a0)+
    MOVE.l #$ADDEADAD,(a0)+
    MOVE.l #$DAEAD987,(a0)+
    MOVE.l #$DEADDEAD,(a1)+
    MOVE.l #$EADDEADA,(a1)+
    MOVE.l #$ADDEADAD,(a1)+
    MOVE.l #$DAEAD987,(a1)+
CELSE
    ADD.w #16,A0
CEND

    MOVE.l A0,D0
 'l1
  MOVEM.l (A7)+,A0-A1/A6
  RTS

 'chipm
  MOVE.l mempoolchip,a0
  JSR _AllocPooled(a6)
  TST.l D0 : BEQ.w outOfMemory

  MOVEA.l D0,A0
  MOVEA.l memlist(PC),A1
  ADDQ.l #4,a1
  !addtail
  ;MOVE.l $4(A1),(A0)
  ;MOVE.l A0,$4(A1)
  MOVE.l (A7)+,12(A0)  ;was 4
  MOVE.l #"ChIp",8(a0)

CNIF #mungwall=1
  MOVE.l a0,a1
  SUB.w #16,a1
  ADD.l 4(a0),a1
  ADD.w #16,a0
  ;MOVE.l #$DEADDEAD,(a0)+
  MOVE.l 28(a7),(a0)+
  MOVE.l #$EADDEADA,(a0)+
  MOVE.l #$ADDEADAD,(a0)+
  MOVE.l #$DAEAD987,(a0)+
  MOVE.l #$DEADDEAD,(a1)+
  MOVE.l #$EADDEADA,(a1)+
  MOVE.l #$ADDEADAD,(a1)+
  MOVE.l #$DAEAD987,(a1)+
CELSE
  ADD.w #16,A0
CEND

  MOVE.l A0,D0
  MOVEM.l (A7)+,A0-A1/A6
RTS

.codeinit:                    ; AJL_0_F0
  CLR.l memlist
  MOVE.l d2,-(a7)

  JSR check_kickstart

  TST.w kick2_available : BEQ 'nopooling
    MOVEA.l _SysBase,a6
    MOVE.l #$10000,d0         ;#MEMF_CLEAR
    MOVE.l #32768,d1          ;alloc pool with 32kb for fastram
    MOVE.l #32768,d2
    JSR _CreatePool(a6)
    MOVE.l d0,mempool
    TST.l D0 : BEQ.w outOfMemory

    MOVE.l #$10002,d0         ;#MEMF_CLEAR & #MEMF_CHIP
    MOVE.l #32768,d1          ;alloc pool with 32kb for chipmem
    MOVE.l #32768,d2
    JSR _CreatePool(a6)
    MOVE.l d0,mempoolchip
    TST.l D0 : BEQ.w outOfMemoryc

    CMP.l #"OuT ",errorhandling : BNE 'l10
      MOVE.b #1,debug
   'l10
 'nopooling

  BSR.w do_newmemnode
  MOVE.l #call_localalloc,D0
  MOVE.l (a7)+,d2
  CLR.w AL_0_EE
RTS

mempool: Dc.l 0
mempoolchip: Dc.l 0


.codefinit:                   ; free all allocated memnodes
;CNIF #mungwall=0
  TST.w kick2_available
  BNE 'nocleanupnecessary
    BSR.w do_freememnode
    BNE.w codefinit
 'nocleanupnecessary
;CEND

  ;-- delete allocated memory pools
  TST.w kick2_available
  BEQ 'nopooling
    MOVE.l a6,-(a7)
    MOVEA.l _SysBase,A6
    MOVE.l mempool,a0 : JSR _DeletePool(A6)
    MOVE.l mempoolchip,a0 : JSR _DeletePool(A6)
    MOVE.l (a7)+,a6
    BRA 'finitend
 'nopooling
 'finitend
RTS

.check_kickstart:
  MOVEQ.l #0,d0
  MOVEA.l _SysBase,A0
  MOVE.w $14(A0),d0           ; Exec.libVersion
  CMP.w #37,d0 : BCS 'nokick2
    MOVE #1,kick2_available
 'nokick2
RTS
kick2_available: Dc.w 0

call_localalloc:  BRA.w do_localalloc
call_localfree:   BRA.w do_localfree
call_globalalloc: BRA.w do_globalalloc
call_globalfree:  BRA.w do_globalfree

.findglobal:                    ; JL_0_122
  TST.l (A0) : BEQ.w JL_0_12E
    MOVEA.l (A0),A0
    BRA.w findglobal            ; JL_0_122
  JL_0_12E:
  MOVE.l A0,memlist
RTS

.do_globalalloc                    ; AJL_0_136
  MOVE.l A0,-(A7)
  MOVEA.l memlist(PC),A0
  MOVE.l A0,-(A7)
  BSR.w findglobal
  BSR.w do_localalloc              ; AJL_0_16A
  MOVE.l (A7)+,memlist
  MOVEA.l (A7)+,A0
RTS

.do_globalfree                     ; AJL_0_150
  MOVE.l A0,-(A7)
  MOVEA.l memlist(PC),A0
  MOVE.l A0,-(A7)
  BSR.w findglobal
  BSR.w do_localfree               ; AJL_0_19E
  MOVE.l (A7)+,memlist
  MOVEA.l (A7)+,A0
RTS

.do_localalloc:                    ;allocate some memory in the local region!    AJL_0_16A
  MOVEM.l A0-A1/A6,-(A7)

  BTST #0,D0 : BEQ 'notodd         ; check if memory is odd and add 1 byte
    ADDQ.l #1,D0
 'notodd

CNIF #mungwall=1
  ADD.l #16+16*2,d0
CELSE
