NPrint "clearing list ..."
ClearList xyz()
NPrint "used mem: ", fm - AvailMem_(0)
NPrint "The End"
End

.filllist:
  ResetList xyz()
  NPrint "filling list..."
  For i = 1 To 8000
    success = AddLast(xyz())
    If success Then xyz() = "Test " + Str$(i)
  Next
  NPrint "added items: ", CountList(xyz())
Return

.infolist:
  ResetList xyz()
  While NextItem(xyz())
    NPrint xyz()
  Wend
Return
