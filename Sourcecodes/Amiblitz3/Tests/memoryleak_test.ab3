OPTIMIZE 13

NPrint "startup and DIMing list..."
fm.l = AvailMem_(0)
NPrint "freemem: ", fm
                                                                                                                                                                                                                                                       
Dim List xyz.s(0)
NPrint "used mem: ", fm - AvailMem_(0)

Gosub filllist
NPrint "used mem: ", fm - AvailMem_(0)

NPrint "clearing list ..."
ClearList xyz()
NPrint "used mem: ", fm - AvailMem_(0)

Gosub filllist
