      *ni\do_DefaultTool = &idefToolTtype
      *ni\do_CurrentX = #NO_ICON_POSITION ; 92
      *ni\do_CurrentY = #NO_ICON_POSITION ; 14

    If PutDiskObject_(filen, *ni) = False
      NPrint "Could not write DiskObject, error " , Str$(IoErr_)

    EndIf
    FreeDiskObject_(*ni)
  Else
    NPrint "Could not init Diskobject structure"
  EndIf

End



