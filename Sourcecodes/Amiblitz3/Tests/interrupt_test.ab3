#INTENA = $DFF09A  ;interupt enable write address
#INTENAR= $DFF01C ;interupt enable read address

#INTREQ = $DFF09C  ;interupt request write address
#INTREQR= $DFF01E ;interupt request read address
#DMACON = $DFF096

Gosub checkints

Poke.w #DMACON, $81c0
Poke.w #INTENA, $8040
Poke.w #INTREQ, $8040

DEFTYPE.l kk

SetInt 6
  kk + 1
End SetInt

Gosub checkints

NPrint "start"
For i.l = 0 To 2000
  If kk<> 0 Then NPrint i, " ", kk
  VWait 1
Next i
NPrint "end"

End

checkints:
For j = 15 To 0 Step -1
  Print Right$(Hexb$(j),1)
Next j
NPrint ""

NPrint Binw$(Peek.w(#INTENAR))
Return
