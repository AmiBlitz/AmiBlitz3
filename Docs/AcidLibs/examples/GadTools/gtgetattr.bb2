WbToScreen 3
Use Screen 3

LoadFont 2,"garnet.font",9
Use IntuiFont 2

foo.l=GTCycle(0,1,20,20,150,15,"Normal",#PLACETEXT_RIGHT,"foo|bar|snafu")
NPrint Hex$(foo)

AddIDCMP #CYCLEIDCMP

Window 0,0,0,400,300,$100f,"jjlk",-1,-1
DefaultOutput

AttachGTList 0,0
While ev.l<>#IDCMP_CLOSEWINDOW
    ev.l=WaitEvent
    Select ev
        Case #IDCMP_GADGETUP
            NPrint "Cycle gadget ",GadgetHit," changed to option ",GTGetAttrs(0,1,#GTCY_Active)
    End Select
Wend
DetachGTList 0
End

