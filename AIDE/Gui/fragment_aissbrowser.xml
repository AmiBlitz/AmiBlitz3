<vgroup title="AISS" image="aiss" border="none">
  <hgroup fixheight="true">
    <imageview image="filter" padding=0 margin=0 id="obj_aiss_filter_image">
    <string hint="filter..." id="obj_aiss_filter" onpostkey="ev_aiss_refresh"/>
    <string hint="<default AISS path>" id="obj_aiss_path" onsubmit="ev_aiss_rescan"/>
    <toolbutton image="refresh" id="obj_aiss_rescan" onclick="ev_aiss_rescan"/>
  </hgroup>
  <listview id="obj_aiss_list" title=""/>
</vgroup>
