<engine 
  arexxport      = "AIDE_REXX"
  appiconfile    = "AIDE"
  appicontext    = "AIDE"
  appname        = "AIDE"
  appmenutext    = "AIDE"
  borderstyle    = 1
  fillstyle      = 1
  onpopup        = "POPUP"
  oniconify      = "ICONIFY"
  onmessage      = "AREXX"
  useaiss        = "false"
  buffereddraw   = "true"
  normalfontface = ""
  normalfontsize = 0
  defpadding     = "2"
  pubscreenname  = ""
  imagepath      = "Gui/images"
/>

<window id="win_splash" borderless=true bgpen="transparent">
  <vgroup>
     <imageview image="Gui/logo.png"/>
     <hgroup bgpen="shadow" gradient="horizontal" border="flat">
       <label text="initializing..." align="right" fixwidth="false" id="obj_splash_message" fgpen="shine"/>
       <imageview image="busy" id="obj_splash_progress" size="native" padding="0" margin="0"/>
     </hgroup>
  </vgroup>
</window>
