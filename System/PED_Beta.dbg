0002675A <PED.ab2@500>       zeile$ = file_ReadLine{fid}
000267AC <PED.ab2@502>       pos_name.l  = Instr(zeile$,"\\09",1)
000267D8 <PED.ab2@503>       If pos_name=0 Then pos_name=FLen(zeile$)+1
0002682E <PED.ab2@505>       pos_desc.l  = Instr(zeile$,"\\09",pos_name+1)
0002685A <PED.ab2@506>       If pos_desc=0 Then pos_desc=FLen(zeile$)+1
000268B6 <PED.ab2@508>       typ$  = Mid$(zeile$,1,pos_name-1)
0002691C <PED.ab2@509>       name$ = Mid$(zeile$,pos_name+1,pos_desc-pos_name-1)
00026972 <PED.ab2@510>       help$ = Mid$(zeile$,pos_desc+1)
000269AA <PED.ab2@513>       If typ$><""
000269E2 <PED.ab2@514>       If typ$ = "FILE"
00026A16 <PED.ab2@515>         filename$ = name$
00026A1C <PED.ab2@516>       Else
00026A68 <PED.ab2@518>         lname$ = LCase$(name$)
00026AA0 <PED.ab2@519>         If name$><""
00026AD8 <PED.ab2@520>           If help$=""
00026B0E <PED.ab2@521>             Select typ$
00026B5A <PED.ab2@522>               Case "FUNC" : help$ = "{...} ; Function"
00026BAC <PED.ab2@523>               Case "MACR" : help$ = "{...} ; Macro"
00026BFE <PED.ab2@524>               Case "STAT" : help$ = "{...} ; Statement"
00026C04 <PED.ab2@525>               Default
00026C38 <PED.ab2@526>                 help$ = typ$
00026C46 <PED.ab2@527>             End Select
00026C58 <PED.ab2@529>           If AddItem(HelpIndexCache())
00026C92 <PED.ab2@530>             HelpIndexCache()\name  = name$
00026CD0 <PED.ab2@531>             HelpIndexCache()\lname = lname$
00026D0E <PED.ab2@532>             HelpIndexCache()\help  = help$
00026D4C <PED.ab2@533>             HelpIndexCache()\includefile = filename$
00026D52 <PED.ab2@538>     Wend
