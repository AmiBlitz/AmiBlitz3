00026C82 <PED.ab2@528>         lname$ = LCase$(name$)
00026CBA <PED.ab2@529>         If name$><""
00026CF2 <PED.ab2@530>           If help$=""
00026D28 <PED.ab2@531>             Select typ$
00026D74 <PED.ab2@532>               Case "FUNC" : help$ = "{...} ; Function"
00026DC6 <PED.ab2@533>               Case "MACR" : help$ = "{...} ; Macro"
00026E18 <PED.ab2@534>               Case "STAT" : help$ = "{...} ; Statement"
