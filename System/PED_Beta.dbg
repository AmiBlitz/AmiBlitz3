00027B6E <PED.ab2@501> Statement TED_LoadHelpIndexFile{HelpIndexFile$}
00027BB2 <PED.ab2@504>   fid.l = file_Open{HelpIndexFile$,#file_read}
00027BC6 <PED.ab2@505>   If fid<>-1
00027BD0 <PED.ab2@506>     ClearList HelpIndexCache()
00027BEC <PED.ab2@508>     While file_EOF{fid}=False
00027C20 <PED.ab2@509>       zeile$ = file_ReadLine{fid}
00027C72 <PED.ab2@511>       pos_name.l  = Instr(zeile$,"\\09",1)
00027C9E <PED.ab2@512>       If pos_name=0 Then pos_name=FLen(zeile$)+1
00027CF4 <PED.ab2@514>       pos_desc.l  = Instr(zeile$,"\\09",pos_name+1)
00027D20 <PED.ab2@515>       If pos_desc=0 Then pos_desc=FLen(zeile$)+1
00027D7C <PED.ab2@517>       typ$  = Mid$(zeile$,1,pos_name-1)
00027DE2 <PED.ab2@518>       name$ = Mid$(zeile$,pos_name+1,pos_desc-pos_name-1)
00027E38 <PED.ab2@519>       help$ = Mid$(zeile$,pos_desc+1)
00027E70 <PED.ab2@522>       If typ$><""
00027EA8 <PED.ab2@523>       If typ$ = "FILE"
00027EDC <PED.ab2@524>         filename$ = name$
00027EE2 <PED.ab2@525>       Else
00027F2E <PED.ab2@527>         lname$ = LCase$(name$)
00027F66 <PED.ab2@528>         If name$><""
00027F9E <PED.ab2@529>           If help$=""
00027FD4 <PED.ab2@530>             Select typ$
00028020 <PED.ab2@531>               Case "FUNC" : help$ = "{...} ; Function"
00028072 <PED.ab2@532>               Case "MACR" : help$ = "{...} ; Macro"
000280C4 <PED.ab2@533>               Case "STAT" : help$ = "{...} ; Statement"
000280CA <PED.ab2@534>               Default
000280FE <PED.ab2@535>                 help$ = typ$
0002810C <PED.ab2@536>             End Select
0002811E <PED.ab2@538>           If AddItem(HelpIndexCache())
00028158 <PED.ab2@539>             HelpIndexCache()\name  = name$
00028196 <PED.ab2@540>             HelpIndexCache()\lname = lname$
000281D4 <PED.ab2@541>             HelpIndexCache()\help  = help$
00028212 <PED.ab2@542>             HelpIndexCache()\includefile = filename$
00028218 <PED.ab2@547>     Wend
00028222 <PED.ab2@548>     file_Close{fid}
0002823A <PED.ab2@552> End Statement
