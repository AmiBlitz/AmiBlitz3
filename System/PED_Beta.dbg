000266E4 <PED.ab2@490> Statement TED_LoadHelpIndexFile{HelpIndexFile$}
00026728 <PED.ab2@493>   fid.l = file_Open{HelpIndexFile$,#file_read}
0002673C <PED.ab2@494>   If fid<>-1
00026746 <PED.ab2@495>     ClearList HelpIndexCache()
00026762 <PED.ab2@497>     While file_EOF{fid}=False
00026796 <PED.ab2@498>       zeile$ = file_ReadLine{fid}
000267E8 <PED.ab2@500>       pos_name.l  = Instr(zeile$,"\\09",1)
00026814 <PED.ab2@501>       If pos_name=0 Then pos_name=FLen(zeile$)+1
0002686A <PED.ab2@503>       pos_desc.l  = Instr(zeile$,"\\09",pos_name+1)
00026896 <PED.ab2@504>       If pos_desc=0 Then pos_desc=FLen(zeile$)+1
000268F2 <PED.ab2@506>       typ$  = Mid$(zeile$,1,pos_name-1)
00026958 <PED.ab2@507>       name$ = Mid$(zeile$,pos_name+1,pos_desc-pos_name-1)
000269AE <PED.ab2@508>       help$ = Mid$(zeile$,pos_desc+1)
000269E6 <PED.ab2@511>       If typ$><""
00026A1E <PED.ab2@512>       If typ$ = "FILE"
00026A52 <PED.ab2@513>         filename$ = name$
00026A58 <PED.ab2@514>       Else
00026AA4 <PED.ab2@516>         lname$ = LCase$(name$)
00026ADC <PED.ab2@517>         If name$><""
00026B14 <PED.ab2@518>           If help$=""
00026B4A <PED.ab2@519>             Select typ$
00026B96 <PED.ab2@520>               Case "FUNC" : help$ = "{...} ; Function"
00026BE8 <PED.ab2@521>               Case "MACR" : help$ = "{...} ; Macro"
00026C3A <PED.ab2@522>               Case "STAT" : help$ = "{...} ; Statement"
00026C40 <PED.ab2@523>               Default
00026C74 <PED.ab2@524>                 help$ = typ$
00026C82 <PED.ab2@525>             End Select
00026C94 <PED.ab2@527>           If AddItem(HelpIndexCache())
00026CCE <PED.ab2@528>             HelpIndexCache()\name  = name$
00026D0C <PED.ab2@529>             HelpIndexCache()\lname = lname$
00026D4A <PED.ab2@530>             HelpIndexCache()\help  = help$
00026D88 <PED.ab2@531>             HelpIndexCache()\includefile = filename$
00026D8E <PED.ab2@536>     Wend
00026D98 <PED.ab2@537>     file_Close{fid}
