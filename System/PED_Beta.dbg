00026E1E <PED.ab2@532>               Case "FUNC" : help$ = "{...} ; Function"
00026E70 <PED.ab2@533>               Case "MACR" : help$ = "{...} ; Macro"
00026EC2 <PED.ab2@534>               Case "STAT" : help$ = "{...} ; Statement"
00026EC8 <PED.ab2@535>               Default
00026EFC <PED.ab2@536>                 help$ = typ$
00026F0A <PED.ab2@537>             End Select
00026F1C <PED.ab2@539>           If AddItem(HelpIndexCache())
