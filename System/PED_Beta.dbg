000266A8 <PED.ab2@491> Statement TED_LoadHelpIndexFile{HelpIndexFile$}
000266EC <PED.ab2@494>   fid.l = file_Open{HelpIndexFile$,#file_read}
00026700 <PED.ab2@495>   If fid<>-1
0002670A <PED.ab2@496>     ClearList HelpIndexCache()
